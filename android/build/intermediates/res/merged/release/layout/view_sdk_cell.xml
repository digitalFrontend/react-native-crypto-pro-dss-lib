<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/view_sdk_cell_vg_main"
    android:layout_marginBottom="8dp"
    android:layout_marginTop="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp">

    <com.digt.sdk.ui.components.SdkSnippet
        android:id="@+id/view_sdk_cell_snippet"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:minHeight="@dimen/sdk_min_height" />
    <ImageButton
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/view_sdk_cell_btn_menu"
        android:src="@drawable/ic_sdk_more_vert_24px"
        android:background="?android:attr/selectableItemBackground"/>
    <Button
        android:visibility="invisible"
        android:enabled="false"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:id="@+id/view_sdk_cell_btn_show_raw"/>
</RelativeLayout>