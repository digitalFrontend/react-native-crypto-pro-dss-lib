<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/ac_sdk_enter_password_dialog_vg_main">
    <LinearLayout
        android:id="@+id/ac_sdk_enter_password_dialog_edit_container"
        android:layout_width="@dimen/tablet_view_width"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"/>
    <com.digt.sdk.ui.components.SdkButtonFooter
        android:layout_centerHorizontal="true"
        android:id="@+id/ac_sdk_enter_password_dialog_footer"
        android:layout_width="@dimen/tablet_view_width"
        android:layout_height="@dimen/sdk_footer_height"
        android:layout_alignParentBottom="true"/>
</RelativeLayout>